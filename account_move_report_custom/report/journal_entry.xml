'.format(debit_line.debit)"/>                                </td>                                <td width="12.5%"                                    style="border:none; text-align:center;"                                    colspan="3">                                </td>                                <td                                        style="border:none;text-align:center;">                                </td>                            </tr>                            <tr style="border:none;" t-foreach="o.line_ids.filtered(lambda line:line.credit > 0)"                                t-as="credit_line">                                <td width="14.5%"                                    style="border:none;text-align:right;">                                    <t t-esc="credit_line.account_id.code"/>                                </td>                                <td width="14.5%"                                    style="border:none; text-align:center;">                                    <t t-esc="credit_line.account_id.name"/>                                </td>                                <td width="12.5%" style="border:none; text-align:center;color:#2e6ead;">                                    <t t-esc="credit_line.currency_id.name"/>                                </td>                                <td width="12.5%"                                    style="border:none; text-align:center;font-size:13px;">                                    <t t-esc="'{:,.2f}'.format(credit_line.credit * -1)"/>                                </td>                                <td width="12.5%" colspan="3">                                </td>                                <td>                                </td>                            </tr>                            <tr style="border:none;">                                <td width="14.5%"                                    style="border:none;text-align:right;" colspan="2">                                </td>                                <td width="12.5%"                                    style="border:none;text-align:right;">                                </td>                                <td width="12.5%"                                    style="border:none; border-bottom:2px solid black !important; text-align:center;font-size:13px;">                                    مدين                                </td>                                <td width="12.5%"                                    style="border:none; border-bottom:2px solid black !important; text-align:center;font-size:13px;">                                    دائن                                </td>                                <td width="12.5%"                                    style="border:none; border-bottom:2px solid black !important; text-align:center;"                                    colspan="2">                                </td>                                <td style="border:none; border-bottom:2px solid black !important; text-align:center;">                                </td>                            </tr>                            <tr style="border:none;">                                <td width="14.5%"                                    style="border:none;text-align:right;" colspan="2">                                </td>                                <td width="12.5%"                                    style="border:none;text-align:center;color:#2e6ead;">                                    <t t-esc="o.currency_id.name"/>                                </td>                                <td width="12.5%"                                    style="border:none; text-align:center;font-size:13px;">                                    <t t-esc="'{:,.2f}'.format(sum(o.line_ids.filtered(lambda line:line.debit > 0).mapped('debit')))"/>                                </td>                                <td width="12.5%"                                    style="border:none; text-align:center;font-size:13px;">                                    <t t-esc="'{:,.2f}'.format(sum(o.line_ids.filtered(lambda line:line.credit > 0).mapped('credit')))"/>                                </td>                                <td width="12.5%"                                    style="border:none;"                                    colspan="2">                                </td>                                <td                                        style="border:none;">                                </td>                            </tr>                        </table>                    </div>                </div>            </t>        </t>    </template></odoo>
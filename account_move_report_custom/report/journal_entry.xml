<odoo>    <template id="journal_entry_report_template">        <t t-call="web.basic_layout">            <t t-set="total_page" t-value="len(docs)"/>            <t t-set="page_number" t-value="0"/>            <t t-foreach="docs" t-as="o">                <t t-set="page_number" t-value="page_number + 1"/>                <div class="page">                    <div class="header" style="direction:rtl; font-size:14px;">                        <table style="border:none; width:100%">                            <tr style="border:none;">                                <td width="33%" style="border:none;text-align:right;vertical-align: middle;">                                    <div style="height:100px;width:75px;display:inline-block;"/>                                    <div style="display:inline-block;">                                        <img src="/account_move_report_custom/static/src/img/sc_header.jpg"                                             style="height:100px;width:100px; vertical-align:bottom;"/>                                    </div>                                    <br/>                                    <span style="font-size:12px;">                                        مؤسسة الشيخ جاسم بن محمد بن ثانى للرعاية الاجتماعية                                    </span>                                </td>                                <td width="34%" style="border:none;text-align:center;vertical-align: middle;">                                    <h5 t-if="o.payment_id">                                        <t t-if="o.payment_id.payment_type == 'outbound'">                                            سند صرف                                            <span t-esc="o.payment_id.payment_method_line_id.name"/>                                        </t>                                        <t t-elif="o.payment_id.payment_type == 'inbound'">                                            سند قبض                                    
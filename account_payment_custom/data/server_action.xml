<odoo>    <data>        <record model="ir.actions.server" id="action_print_personal_cheque">            <field name="name">Personal Cheque</field>            <field name="model_id" ref="account.model_account_payment"/>            <field name="binding_model_id" ref="account.model_account_payment"/>            <field name="binding_type">report</field>            <field name="state">code</field>            <field name="code">                action = records.print_personal_cheque()            </field>        </record>        <record model="ir.actions.server" id="action_print_company_cheque">            <field name="name">Company Cheque</field>            <field name="model_id" ref="account.model_account_payment"/>            <field name="binding_model_id" ref="account.model_account_payment"/>            <field name="binding_type">report</field>            <field name="state">code</field>            <field name="code">                action = records.print_company_cheque()            </field>        </record>    </data></odoo>
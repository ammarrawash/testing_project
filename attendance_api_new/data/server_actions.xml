<odoo>    <data>        <record id="set_state_success_action_server" model="ir.actions.server">            <field name="name">Set state Success</field>            <field name="type">ir.actions.server</field>            <field name="model_id" ref="model_machine_attendance_record"/>            <field name="binding_model_id" ref="model_machine_attendance_record"/>            <field name="groups_id" eval="[(4,ref('base.group_erp_manager'))]"/>            <field name="state">code</field>            <field name="code">                if records:                records.set_state_success()            </field>        </record>        <record id="set_state_failure_action_server" model="ir.actions.server">            <field name="name">Set state failure</field>            <field name="type">ir.actions.server</field>            <field name="model_id" ref="model_machine_attendance_record"/>            <field name="binding_model_id" ref="model_machine_attendance_record"/>            <field name="groups_id" eval="[(4,ref('base.group_erp_manager'))]"/>            <field name="state">code</field>            <field name="code">                if records:                records.set_state_failure()            </field>        </record>        <record id="set_state_skip_action_server" model="ir.actions.server">            <field name="name">Set state skip</field>            <field name="type">ir.actions.server</field>            <field name="model_id" ref="model_machine_attendance_record"/>            <field name="binding_model_id" ref="model_machine_attendance_record"/>            <field name="groups_id" eval="[(4,ref('base.group_erp_manager'))]"/>            <field name="state">code</field>            <field name="code">                if records:                records.set_state_skip()            </field>        </record>    </data></odoo>
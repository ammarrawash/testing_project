<odoo>    <record id="delete_un_successful_requests_action_server" model="ir.actions.server">        <field name="name">Delete UnSuccessful Requests</field>        <field name="type">ir.actions.server</field>        <field name="model_id" ref="base_dms_integration.model_dms_integration"/>        <field name="binding_model_id" ref="base_dms_integration.model_dms_integration"/>        <field name="groups_id" eval="[(4,ref('base.group_system'))]"/>        <field name="state">code</field>        <field name="code">            if records:            records.delete_unsuccessful_requests()        </field>    </record>    <record id="dms_temporary_old_data_action_server" model="ir.actions.server">        <field name="name">DMS Temporary Old Data</field>        <field name="type">ir.actions.server</field>        <field name="model_id" ref="base_dms_integration.model_dms_integration"/>        <field name="binding_model_id" ref="base_dms_integration.model_dms_integration"/>        <field name="groups_id" eval="[(4,ref('base.group_system'))]"/>        <field name="state">code</field>        <field name="code">            if records:            records.dms_temporary_old_data()        </field>    </record></odoo>
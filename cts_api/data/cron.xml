<odoo>    <record forcecreate="True" id="ir_cron_send_fail_po_cts_attachment" model="ir.cron">        <field name="name">Purchase: Send Fail CTS Attachment</field>        <field name="model_id" ref="purchase.model_purchase_order"/>        <field name="state">code</field>        <field name="code">model.send_fail_attachment()</field>        <field name="user_id" ref="base.user_root"/>        <field name="interval_number">1</field>        <field name="interval_type">days</field>        <field name="numbercall">-1</field>        <field name="doall" eval="False"/>    </record>    <record forcecreate="True" id="ir_cron_send_fail_account_move_cts_attachment" model="ir.cron">        <field name="name">Account Move: Send Fail CTS Attachment</field>        <field name="model_id" ref="account.model_account_move"/>        <field name="state">code</field>        <field name="code">model.send_fail_attachment()</field>        <field name="user_id" ref="base.user_root"/>        <field name="interval_number">1</field>        <field name="interval_type">days</field>        <field name="numbercall">-1</field>        <field name="doall" eval="False"/>    </record></odoo>
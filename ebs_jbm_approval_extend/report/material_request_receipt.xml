<odoo>    <template id="material_request_receipt_report_template">        <t t-call="web.basic_layout">            <div class="page" style="direction: rtl;">                <t t-foreach="docs" t-as="doc">                    <t t-set="doc" t-value="doc.sudo()"/>                    <style type="text/css">                        @font-face {                        font-family: 'Parastoo-normal';                        src: local('Parastoo'), local('Parastoo'),                        url(/ebs_jbm_approval_extend/static/src/font/Parastoo Print Regular.ttf) format('truetype');                        }                        @font-face {                        font-family: 'Parastoo-Bold';                        src: local('Parastoo'), local('Parastoo'),                        url(/ebs_jbm_approval_extend/static/src/font/Parastoo-Bold.ttf) format('truetype');                        }                        .Bold {                        font-family: Parastoo-Bold;                        }                        body{                        font-family: 'Parastoo-normal', serif !important;                        font-size:14px!important;                        }                        .table                        {                        margin-bottom:0px;                        }                        .table tr                        {                        line-height:5px!important;                        }                        td{                        border-right: 1px solid #c8bca8!important;                        border-left: 1px solid #c8bca8!important;                        border-bottom: 1px solid #c8bca8!important;                        }                        .custom_td{                        text-align: right!important;                        vertical-align: middle;                        }                        .custom_input{                        width:20px;                        height:20px;                        }                        .row{                        text-align:right;                        }                    </style>                    <body DIR="RTL">                        <t t-set="o" t-value="doc.material_request_id"/>                        <div class="header" style="text-align:center;">                            <img t-if="doc.company_id.company_report_header"                                 t-att-src="image_data_uri(doc.company_id.company_report_header)"                                 style="padding-left:5px;height:147px;width:100%;" alt="Logo"/>                        </div>                        <br/>                        <br/>                        <br/>                        <h3 style="text-decoration: underline; Text-align:center">سند صرف داخلي</h3>                        <br/>                        <br/>                        <div class="row">                            <div class="col-3">                                <span style="font-size:20px!important;">التاريخ :</span>                            </div>                            <div class="col-9" style="font-size:20px!important;">                                <t t-esc="doc.date_done and doc.date_done.date()"/>                            </div>                        </div>                        <br/>                        <div class="row">                            <div class="col-3">                                <span style="font-size:20px!important;">رقم السند :</span>                            </div>                            <div class="col-9" style="font-size:20px!important;">                                <t t-esc="doc.name"/>                            </div>                        </div>                        <br/>                        <div class="row">                            <div class="col-3">                                <span style="font-size:20px!important;">رقم طلب الاحتياج :</span>                            </div>                            <div class="col-9" style="font-size:20px!important;">                                <t t-esc="o.name"/>                            </div>                        </div>                        <br/>                        <div class="row">                            <div class="col-12">                                <span style="font-size:20px!important;">استلمت أنا الموقع أدناه بمقر                                    المؤسسة لاستخدامات العمل ما هو مذكور أدناه:                                </span>                            </div>                        </div>                        <br/>                        <table class="table" style="border: 1px solid #c8bca8!important;">                            <tr style="background-color: #0d005f!important;">                                <td style="color: white;text-align: center;" class="Bold" width="2%">                                    م                                </td>                                <td style="color: white;text-align: center;" class="Bold" width="35%">                                    البيان / المواصفات                                </td>                                <td style="color: white;text-align: center;" class="Bold" width="25%">                                    الرقم التسلسلى                                </td>                                <td style="color: white;text-align: center;" class="Bold" width="19%">                                    الوحدة                                </td>                                <td style="color: white;text-align: center;" class="Bold" width="19%">                                    الكمية                                </td>                            </tr>                            <t t-set="sr_number" t-value="0"/>                            <tr t-foreach="doc.move_line_ids_without_package" t-as="line">                                <t t-set="sr_number" t-value="sr_number +1"/>                                <td width="2%" class="custom_td">                                    <span t-esc="sr_number"/>                                </td>                                <td width="35%" class="custom_td">                                    <span t-esc="line.product_id.name"/>                                </td>                                <td width="25%" class="custom_td">                                    <span t-esc="line.lot_id.name"/>                                </td>                                <td width="19%" class="custom_td">                                    <span t-esc="line.product_uom_id.name"/>                                </td>                                <td width="19%" class="custom_td">                                    <span t-esc="line.qty_done"/>                                </td>                            </tr>                        </table>                        <br/>                        <br/>                        <br/>                        <div class="row">                            <div class="col-12">                                <span style="font-size:20px!important;  font-weight:bold;">                                    بيانات المستلـم                                </span>                            </div>                        </div>                        <br/>                        <t t-set="partner_related_user" t-value="doc.get_partner_related_user_name()"/>                        <div class="row">                            <div class="col-3">                                <span style="font-size:20px!important;">الاسم:</span>                            </div>                            <div class="col-9" style="font-size:20px!important;">                                <t t-esc="partner_related_user.name"/>                            </div>                        </div>                        <br/>                        <div class="row">                            <div class="col-3">                                <span style="text-align:left;font-size:20px!important;vertical-align: middle;">                                    التوقيع:                                </span>                            </div>                            <div class="col-9" style="font-size:20px!important;">                                <img width="150" height="75" t-if="partner_related_user.sudo().sign_signature"                                     t-attf-src="data:image/*;base64,{{partner_related_user.sudo().sign_signature}}"                                     />                            </div>                        </div>                        <div class="footer">                                <img src="/ebs_jbm_approval_extend/static/src/img/stock_picking_footer.jpg"                                     style="height:90px;width:100%;"/>                        </div><!--                        <div class="footer">--><!--                            <img t-if="doc.company_id.company_report_footer"--><!--                                 t-att-src="image_data_uri(doc.company_id.company_report_footer)"--><!--                                 style="width:100%" alt="Letter Foot"/>--><!--                        </div>-->                    </body>                </t>            </div>        </t>    </template>    <record id="material_request_receipt_report_action" model="ir.actions.report">        <field name="name">سند صرف داخلي</field>        <field name="model">stock.picking</field>        <field name="report_type">qweb-pdf</field>        <field name="report_name">ebs_jbm_approval_extend.material_request_receipt_report_template</field>        <field name="report_file">ebs_jbm_approval_extend.material_request_receipt_report_template</field>        <field name="binding_model_id" eval="False"/>        <field name="binding_type">report</field>    </record></odoo>
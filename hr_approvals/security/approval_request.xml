<odoo>    <data>        <function name="write" model="ir.model.data">            <function name="search" model="ir.model.data">                <value eval="[('module', '=', 'approvals'), ('name', '=', 'approval_request_approvers_rule')]"/>            </function>            <value eval="{'noupdate': False}"/>        </function>        <record id="approvals.approval_request_approvers_rule" model="ir.rule">            <field name="domain_force">['|', ('approver_ids.user_id.id','=', user.id),                ('approver_ids.group_id.users','in', user.ids)]            </field>        </record>        <function name="write" model="ir.model.data">            <function name="search" model="ir.model.data">                <value eval="[('module', '=', 'approvals'), ('name', '=', 'approval_request_approvers_rule')]"/>            </function>            <value eval="{'noupdate': True}"/>        </function>    </data></odoo>